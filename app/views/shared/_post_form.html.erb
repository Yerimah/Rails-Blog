<%= form_with(model: @post, url: user_posts_path) do |form| %>
    <div class="field">
    <%= form.label :title %> <br>
    <%= form.text_field :title %> <br>
    </div>

    <div class="field">
    <%= form.label :text %> <br>
    <%= form.text_area :text, cols: 30, rows: 10 %> <br>
    </div>


    <%= form.submit "Submit", class: "btn" %>
  <% end %>